                            <div id="Left">
                                <uc3:Left ID="Left1" runat="server" />
                            </div>
                            <!--Right-->
                            <div id="content_area" class="block">
                                <div id="headcontent">
                                </div>
                                <!-- Nội dung sản phẩm -->
                                <div class="block_inside">
                                    <uc2:Right ID="Right1" runat="server" />
                                </div>
                                <div id="footcontent">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="ft">
                </div>
            </div>
        </div>
        </div>
    </center>
